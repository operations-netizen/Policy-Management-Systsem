    "cookie": "^1.0.2",
    "dotenv": "^17.2.2",
    "express": "^4.21.2",
    "jose": "6.1.0",
    "mongodb": "^7.0.0",
    "mongoose": "^9.1.4",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.1.5",
    "nodemailer": "^7.0.13",
    "pdfkit": "^0.17.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "esbuild": "^0.25.0",
    "nodemon": "^3.1.11",
    "vite": "^7.1.7",
    "vitest": "^4.0.17"
